{
  "name": "@authnomicon/oauth2",
  "version": "0.0.0",
  "description": "Injectable OAuth 2.0 implementation for Bixby.js.",
  "keywords": [
    "oauth2"
  ],
  "author": {
    "name": "Jared Hanson",
    "email": "jaredhanson@gmail.com",
    "url": "http://www.jaredhanson.net/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/authnomicon/aaa-oauth2.git"
  },
  "bugs": {
    "url": "http://github.com/authnomicon/aaa-oauth2/issues"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "main": "./app",
  "assembly": {
    "namespace": "oauth2",
    "components": [
      "server",
      "code/grant",
      "code/response",
      "code/issue/code",
      "code/tokens/authorization-code/interpret",
      "code/tokens/authorization-code/translate",
      "ext/responsemode",
      "ext/webmessage",
      "ext/modes/formpost",
      "ext/modes/webmessage",
      "implicit/response",
      "implicit/issue",
      "password/grant",
      "util/issuetoken",
      "http/authorization",
      "http/handlers/authorize/processtransaction",
      "http/handlers/authorize/completetransaction",
      "http/token",
      "http/auth/secret/basic",
      "http/auth/secret/post",
      "http/auth/none",
      "http/middleware/errorhandler",
      "http/workflow/authorize",
      "http/workflow/authorize/transition/login",
      "http/workflow/authorize/transition/mfa",
      "http/workflow/authorize/transition/consent"
    ]
  },
  "dependencies": {
    "clone": "^1.0.2",
    "express": "^4.15.3",
    "oauth2orize": "^1.8.0",
    "oauth2orize-response-mode": "^0.1.0",
    "passport-http": "^0.3.0",
    "passport-oauth2-client-password": "^0.1.2",
    "passport-oauth2-client-public": "0.0.1",
    "uid2": "0.0.3",
    "visa": "0.0.1"
  },
  "devDependencies": {
    "make-node": "^0.3.0",
    "mocha": "^4.0.1",
    "chai": "^4.1.1",
    "chai-date-proximity": "0.0.3",
    "sinon-chai": "^2.8.0",
    "proxyquire": "^1.7.0",
    "sinon": "^4.0.2"
  },
  "engines": {
    "node": "*"
  },
  "scripts": {
    "test": "node_modules/.bin/mocha --reporter spec --require test/bootstrap/node test/*.test.js"
  }
}
