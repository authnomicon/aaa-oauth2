{
  "name": "@authnomicon/oauth2",
  "version": "0.0.0",
  "description": "OAuth 2.0 components for the Authnomicon project.",
  "keywords": [
    "oauth2"
  ],
  "author": {
    "name": "Jared Hanson",
    "email": "jaredhanson@gmail.com",
    "url": "http://www.jaredhanson.net/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/authnomicon/oauth2.git"
  },
  "bugs": {
    "url": "http://github.com/authnomicon/oauth2/issues"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "main": "./app",
  "assembly": {
    "namespace": "org.authnomicon/oauth2",
    "components": [
      "server",
      "http/grant/code/exchange",
      "http/grant/code/grant",
      "http/grant/code/issue/code",
      "http/grant/token/grant",
      "http/grant/token/issue",
      "http/exchange/password/exchange",
      "util/issuetoken",
      "http/authorization",
      "http/handlers/authorize/processtransaction",
      "http/handlers/authorize/completetransaction",
      "http/handlers/authorize/modes/formpost",
      "http/handlers/authorize/modes/webmessage",
      "http/handlers/authorize/params/responsemode",
      "http/handlers/authorize/params/webmessage",
      "http/token",
      "http/auth/secret/basic",
      "http/auth/secret/post",
      "http/auth/none",
      "http/middleware/errorhandler",
      "http/ceremony/authorize",
      "tokens/code/schema"
    ]
  },
  "dependencies": {
    "clone": "^1.0.2",
    "express": "^4.15.3",
    "oauth2orize": "^1.8.0",
    "oauth2orize-response-mode": "^0.1.0",
    "passport-http": "^0.3.0",
    "passport-oauth2-client-password": "^0.1.2",
    "passport-oauth2-client-public": "0.0.1",
    "uid2": "0.0.3",
    "visa": "0.0.1"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "chai-date-proximity": "0.0.3",
    "chai-express-handler": "0.0.3",
    "flowstate": "^0.5.0",
    "make-node": "^0.3.0",
    "mocha": "^4.0.1",
    "proxyquire": "^1.7.0",
    "sinon": "^4.0.2",
    "sinon-chai": "^2.8.0"
  },
  "engines": {
    "node": "*"
  },
  "scripts": {
    "test": "node_modules/.bin/mocha --reporter spec --require test/bootstrap/node test/*.test.js"
  }
}
